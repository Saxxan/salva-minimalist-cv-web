---
// Data
import { work } from '@cv';

// UI
import StandardSection from '@/components/ui/StandardSection.astro';
import JobCard from '../ui/JobCard.astro';
import ColumnCardsWrapper from '../ui/ColumnCardsWrapper.astro';
---

<StandardSection title="Experience">
    <ColumnCardsWrapper>
        {work.map((job) => <JobCard name={job.name} position={job.position} url={job.url} start={job.startDate} end={job.endDate} summary={job.summary} />)}
    </ColumnCardsWrapper>
</StandardSection>